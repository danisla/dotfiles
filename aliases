alias k='kubectl'
alias ls='ls --color'
alias ll='ls -l'
alias grep='grep --color=auto'

alias yt-dl='docker run \
    --rm -i \
    -e PGID=$(id -g) \
    -e PUID=$(id -u) \
    -v "$(pwd)":/workdir:rw \
    mikenye/youtube-dl'

# Init ssh-agent with GCE ssh key
alias sagent='sshagent_init; ssh-add ${HOME}/.ssh/google_compute_engine'